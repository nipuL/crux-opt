+GBool HtmlFont::isEqual(const HtmlFont& x) const{
+  return ((size==x.size) &&
+	  (lineSize==x.lineSize) &&
+	  (pos==x.pos) && (bold==x.bold) && (italic==x.italic) &&
+	  (color.isEqual(x.getColor())));
