+char* getFileNameFromPath(char* c, int strlen) {
+  int last_slash_index = 0;
+  int i = 0;
+  char* res;
+  
+  for (i=0;i<strlen;i++) {
+    if (*(c+i)=='/') {
+      /* printf("/ detected\n"); */
+      last_slash_index = i;      
+    }
+  }
+  res = (char *)malloc(sizeof(char)*strlen-last_slash_index+1);
+  strcpy(res,c+last_slash_index+(last_slash_index?1:0));
+  /* printf("Fil: %s\n",res); */
+  return res;
+}
