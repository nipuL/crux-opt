+
+char* HtmlOutputDev::mapEncodingToHtml(GooString* encoding)
+{
+    char* enc = encoding->getCString();
+    for(int i = 0; HtmlEncodings[i][0] != NULL; i++)
+    {
+	if( strcmp(enc, HtmlEncodings[i][0]) == 0 )
+	{
+	    return HtmlEncodings[i][1];
+	}
+    }
+    return enc; 
+}
