+HtmlFontColor::HtmlFontColor(GfxRGB rgb){
+  r=static_cast<int>(255*rgb.r);
+  g=static_cast<int>(255*rgb.g);
+  b=static_cast<int>(255*rgb.b);
+  if (!(Ok(r)&&Ok(b)&&Ok(g))) {printf("Error : Bad color \n");r=0;g=0;b=0;}
